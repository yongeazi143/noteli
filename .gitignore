# Ignore files generated by the operating system
.DS_Store
Thumbs.db

# Ignore directories generated by many tools
node_modules/
bower_components/

# Ignore log and temporary files
*.log
*.swp
*.tmp

# Ignore files generated by build processes
/dist/
/build/
/bin/

# Ignore sensitive or environment-specific information
.env
.env.local
.env.*.local

# Ignore dependency lock files
yarn.lock
package-lock.json

# Ignore user-specific files
*.sublime-project
*.sublime-workspace
*.idea/
*.vscode/

# Ignore macOS folder attributes
.metadata

# Ignore Jekyll generated files
_site/
.sass-cache/
.jekyll-cache/
.jekyll-metadata

# Ignore Gradle build directory
/build/

# Ignore Python virtual environment
venv/
__pycache__/

# Ignore compiled Java classes
*.class

# Ignore .gitignore itself when it's at the root of the repository
/.gitignore
